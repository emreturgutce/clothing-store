{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "quotes": "off",
    "import/prefer-default-export": "off"
  }
}
